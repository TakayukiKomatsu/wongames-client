    ".github": true,
    ".husky": true,
    ".jest": true,
    ".next": true,
    ".storybook": true,
    "coverage": true,
    "generators": true,
    ".editorconfig": true,
    ".eslintignore": true,
    ".eslintrc.json": true,
    ".gitignore": true,
    ".prettierignore": true,
    ".prettierrc": true,
    "jest.config.js": true,
    "next-env.d.ts": true,
    "next.config.js": true,
